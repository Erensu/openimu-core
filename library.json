{
  "name": "openimu-core-libraries",
  "version": "0.0.0",
  "description": "Open Source Embedded IMU and Navigation Library",
  "keywords": "sensors, IMU, INS, AI, automotive, navigation",
  "repository":
  {
    "type": "git",
    "url": "https://github.com/Aceinna/openimu-core.git",
    "branch": "master"
  },  
  "build": {
    "flags": [
      "-I CLI/include",
      "-I BSP/include",
      "-I CSL",
      "-I CSL/include",
      "-I GPS/include",
      "-I Math/include",
      "-I Platform",
      "-I Algorithm/include",
      "-I FreeRtos/include",
      "-I Math",
      "-I UART/include",
      "-I Filter/include",
      "-L Platform",
      "-l libPlatform"      
    ],
    "libArchive": false,
    "platforms": "aceinna_imu"
  }
 }
